<?xml version="1.0" ?>

<!-- this file should be updated to control the search engines 
use attributes ie, firefox and chrome to control which browsers the settings are NOT applied to 
e.g. ie="0" firefox="0" chrome="0"

%s will be substituted for search terms. (In Chrome too)

remember to use &amp; in URLs!   (or ; can work - but &amp; probably more reliable)
-->

<searches>
	<!-- chrome only shortcuts -->
	<search keyword="hi"    title="history"    	ie="0" firefox="0"			url="chrome://history/search#q=%s" 		/>
	<search keyword="bm"    title="bookmarks"    	ie="0" firefox="0"			url="chrome://bookmarks/#q=%s" 			/>
	<search keyword="set"   title="settings"    	ie="0" firefox="0"			url="chrome://settings/search#%s" 		/>
	<search keyword="dn"    title="downloads"    	ie="0" firefox="0"			url="chrome://downloads/#%s" 			/>

	<!-- public urls -->
	<search keyword="w"     title="Wikipedia"    		url="http://en.wikipedia.org/w/index.php?title=Special:Search&amp;search=%s" />
	<search keyword="en" 	title="Wordref Eng" 		url="http://www.wordreference.com/definition/%s" />
	<search keyword="map"   title="Google maps"    		url="https://www.google.com/maps/place/%s" />
	<search keyword="ln"    title="Linked in"    		url="http://www.linkedin.com/search/fpsearch?type=people&amp;pplSearchOrigin=GLHD&amp;pageKey=member-home&amp;search=Search&amp;keywords=%s" public="0"/>
	<search keyword="gi"    title="Google images"    	url="https://www.google.com/search?tbm=isch&amp;q=%s" />
	<search keyword="gsc"    title="Google scholar" 	url="https://scholar.google.co.uk/scholar?hl=en&amp;btnG=&amp;as_sdt=1%2C5&amp;as_sdtp=&amp;q=%s" />	
	<search keyword="gsh"    title="Google shopping" 	url="https://www.google.co.uk/#tbm=shop&amp;q=%s" />	
	<search keyword="gbk"    title="Google books" 		url="https://www.google.co.uk/#tbm=bks&amp;q=%s" />	

	<search keyword="g"     title="Google"    			url="https://www.google.co.uk/search?q=%s" />
	<search keyword="ebay"  title="eBay UK"    			url="http://www.ebay.co.uk/sch/i.html?_nkw=%s&amp;_sacat=0" />
	
	

</searches>





